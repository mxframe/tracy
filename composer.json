{
  "name": "mxframe/tracy",
  "description": "The MxFrame Laravel Tracy Plugin",
  "keywords": [
    "framework",
    "laravel",
    "mxframe",
    "nette",
    "tracy"
  ],
  "license": "MIT",
  "homepage": "https://mx-progress.com/mxframe/tracy",
  "authors": [
    {
      "name": "Mathias Berg",
      "homepage": "https://mx-progress.com",
      "role": "Developer"
    }
  ],
  "type": "library",
  "require": {
    "php": ">=7.1.0",
    "laravel/framework": "^5.1",
    "tracy/tracy": "^2.5.2"
  },
  "require-dev": {
    "mockery/mockery": "~0.9.4|~1.0",
    "phpunit/phpunit": "~4.8|~5.4|~6.1|~7.0",
    "nesbot/carbon": "~1.20"
  },
  "autoload": {
    "files": [
      "src/polyfill.php",
      "src/helpers.php"
    ],
    "psr-4": {
      "MxFrame\\Tracy\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MxFrame\\Tracy\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "MxFrame\\Tracy\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
